import java.util.*;
class Example{
	
	public static int findNum(int a, int b){
		for(int c=2; c<7; c++) {
			a = a+1;
			b=b+1;
			if((a^b) > (a&b)) {
				a = a+1;
				b=b+1;
			}
			b=b+1;
		}
		return a+b;
    }
	
	public static void main(String [] args) {
	     	
		
		System.out.println(findNum(2, 3));
	}
}

