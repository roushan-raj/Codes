import java.util.*;
class Example{
	
	public static int findNum(int [] arr, int n) {
		int count = 0;
		
         for(int i=0; i<n; i++) {
        	 if(arr[i+1] < arr[i]) {
        		 count++;
        	 }
         }
         return count;
    }
	
	public static void main(String [] args) {
		
		int arr[] = {2, 3, 4, 5, 6, 1};
		System.out.println(findNum(arr, arr.length));
	}
}


